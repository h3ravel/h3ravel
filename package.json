{
  "name": "h3ravel",
  "version": "1.0.0-alpha.1",
  "description": "Modern TypeScript runtime-agnostic web framework built on top of H3.",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "categories": [
    "Programming Languages",
    "Extension Packs"
  ],
  "keywords": [
    "h3ravel",
    "framework",
    "nodejs",
    "typescript",
    "laravel"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "dev": "NODE_ENV=development pnpm tsdown --config-loader unconfig -c tsdown.default.config.ts",
    "start": "DIST_DIR=dist node -r source-map-support/register dist/server.js",
    "lint": "eslint . --ext .ts",
    "test": "NODE_NO_WARNINGS=1 NODE_ENV=testing vitest",
    "coverage": "NODE_NO_WARNINGS=1 NODE_ENV=testing vitest run --coverage",
    "cleanup": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "postinstall": "pnpm prepare"
  },
  "dependencies": {
    "@h3ravel/arquebus": "^0.8.0-alpha.6",
    "@h3ravel/cache": "^11.1.0-alpha.10",
    "@h3ravel/config": "^1.5.0-alpha.10",
    "@h3ravel/console": "^11.15.0-alpha.10",
    "@h3ravel/core": "^1.22.0-alpha.10",
    "@h3ravel/database": "^11.5.0-alpha.10",
    "@h3ravel/filesystem": "^0.4.15-alpha.10",
    "@h3ravel/hashing": "^0.2.0-alpha.10",
    "@h3ravel/http": "^11.8.0-alpha.10",
    "@h3ravel/mail": "^11.0.17-alpha.10",
    "@h3ravel/musket": "^0.8.0-alpha.6",
    "@h3ravel/queue": "^11.1.0-alpha.10",
    "@h3ravel/router": "^1.15.0-alpha.10",
    "@h3ravel/shared": "^0.29.0-alpha.10",
    "@h3ravel/support": "^0.17.0-alpha.10",
    "@h3ravel/url": "^1.1.0-alpha.10",
    "@h3ravel/view": "^0.1.14-alpha.10",
    "@h3ravel/validation": "^0.1.0-alpha.10",
    "@h3ravel/foundation": "^0.1.0-alpha.10",
    "@h3ravel/session": "^0.1.0-alpha.10",
    "@h3ravel/events": "^0.1.0-alpha.10",
    "cross-env": "^10.1.0",
    "h3": "2.0.1-rc.5",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "5.1.7"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@rollup/plugin-run": "^3.1.0",
    "@swc/core": "^1.13.5",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.4",
    "eslint": "^9.34.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsdown": "^0.16.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@10.15.0"
}